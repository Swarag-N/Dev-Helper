{
  "manifest_version": 2,
  "name": "Dev Helper",
  "description": "Chrome Extension for Web Developer",
  "version": "0.1.0",
  "author": "Swarag Naryanasetty",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["tabs"]
}
